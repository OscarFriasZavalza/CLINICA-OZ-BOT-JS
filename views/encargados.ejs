        <section>
            <div id="team-section" class="pad-sec">
                <div class="container">
                    <div class="title-section animated out" data-animation="fadeInUp" data-delay="0">
                        <div class="row">
                            <div class="col-sm-8 col-sm-offset-2">
                                <h1 style= "font-family: Arial Rounded MT Bold; color: #027377;">ENCARGADOS <br> REGISTRADOS</h1>
                                <hr>
                                <!-- <P>LOS MEJORES ESPECIALISTAS</P> -->
                            </div>
                        </div> <!-- End row -->
                    </div> <!-- end title-section -->
  
                    <div  class="team-members">    
                     
                        <table class="table table-bordered table-striped text-center mt-4">
                            <thead>
                              <tr class="bg-primary text-white">
                                <th scope="col">#</th>
                                <th scope="col">Nombre</th>
                                <th scope="col">Apellido Paterno</th>     
                                <th scope="col">Apellido Materno</th>   
                                <th scope="col">Sexo</th>
                                <th scope="col">Fecha Nacimiento</th> 
                                <th scope="col">Telefono</th>    
                                <th scope="col">Correo</th>    
                                <th scope="col">Servicio</th>    
                                <th scope="col">Contraseña</th>    
                             
                              </tr>
                            </thead>
                            <tbody>
                              <% results.forEach((cliente)=>{ %>
                                <tr>  
                                  <td><%= cliente.idMedico %></td>  
                                  <td><%= cliente.nombre%></td>
                                  <td><%= cliente.apellidoP %></td>
                                  <td><%= cliente.apellidoM %></td>
                                  <td><%= cliente.sexo %></td> 
                                  <td><%= cliente.fechaNacimiento %></td>
                                  <td><%= cliente.telefono %></td>  
                                  <td><%= cliente.correo %></td>    
                                  <td><%= cliente.idServicio %></td> 
                                  <td><%= cliente.contra %></td> 
                                    <td>
                                        <A href="deleteEncargado/<%= cliente.idMedico%>" class="btn btn-danger"><box-icon name='x-circle' animation='tada' color='#ffffff' ></box-icon></A>               
                                        <A  href="editarLaboratorista/<%= cliente.idMedico%>" class="btn btn-info"><box-icon name='edit-alt' type='solid' animation='tada' color='#ffffff' ></box-icon></A> 
                                    </td>
                                </tr>                                   
                             <% }) %>
                            </tbody>
                          </table>
                    <A HREF="crearLaboratorista" class="btn btn-info">AGREGAR</A> 
                    </div>
                </div> <!-- End container -->
            </div> <!-- End team-section -->
        </section>
