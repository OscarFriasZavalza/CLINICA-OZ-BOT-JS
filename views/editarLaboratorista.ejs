    <!-- Contact-section
    ================================================== -->
    
    <section style="padding: 100px;">
      <% if (encargado) { %>
        <div class="container mt-4">     
        <div class="card border-secondary">  
            <div class="card-header">Nuevo Registro</div>
            <div class="card-body">
          
                <form action="/editarEncargado" method="POST">
                  <input id="idMedico" name="idMedico" value="<%= encargado.idMedico %>" hidden>
                    <div class="mb-3">
                    <input type="text" value="<%= encargado.nombre %>"  placeholder="Nombre" class="form-control" id="nombre" name="nombre" tabindex="1">
                    </div>   
                    <div class="mb-3">
                      <input type="text" value="<%= encargado.apellidoP %>"  placeholder="Apellido Paterno" class="form-control" id="apellidoP" name="apellidoP" tabindex="1">
                    </div>  
                    <div class="mb-3">
                        <input type="text"value="<%= encargado.apellidoM %>"   placeholder="Apellido Materno" class="form-control" id="apellidoM" name="apellidoM" tabindex="1">
                    </div> 
                    <div class="mb-3">
                      <input type="text" value="<%= encargado.sexo %>"   placeholder="Sexo" class="form-control" id="sexo" name="sexo" tabindex="1">
                    </div>
                    <div class="mb-3">
                      <input type="date" value="<%= encargado.fechaNacimiento %>"  placeholder="Fecha de Nacimiento" class="form-control" id="fechaNacimiento" name="fechaNacimiento" tabindex="1">
                    </div>
                    <div class="mb-3">
                      <input type="text" value="<%= encargado.telefono %>"  placeholder="Telefono" class="form-control" id="telefono" name="telefono" tabindex="1">
                    </div>
                    <div class="mb-3">
                      <input type="text" value="<%= encargado.correo %>"  placeholder="Correo" class="form-control" id="correo" name="correo" tabindex="1">
                    </div>
                    <div class="mb-3">
                      <input type="text" value="<%= encargado.contra %>"  placeholder="Contraseña" class="form-control" id="contra" name="contra" tabindex="1">
                    </div>
                    <div class="mb-3">
                      <select id="idServicio" name="idServicio" class="form-select" aria-label="Default select example" tabindex="2">
                        <option value="Laboratorio" selected>Laboratorio</option>
                        <option value="Ultrasonido" selected>Ultrasonidos</option>
                        <option value="Electrocardiograma" selected>Electrocardiograma</option>
                        <option value="Tomografía" selected>Tomografía</option>
                        <option value="RayosX" selected>RayosX</option>
                        <option value="Densitometria" selected>Densitometria</option>
                        <option value="Resonancia" selected>Resonancia Magnetica</option>
                        <option value="Papanicolau" selected>Papanicolau</option>
                        <option value="Mastografia" selected>Mastografia</option>
                        <option value="Nutricion" selected>Nutricion</option>
                    </select>
                    </div>

                    <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                    <a href=".../encargados" class="btn btn-secondary">Cancelar</a>
                </form>
            </div>
        </div>        
  </div>
  <% } %>

    </section>

    
