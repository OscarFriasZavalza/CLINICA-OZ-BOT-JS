            <!--AQUI INICIA EL FORMULARIO-->
            <section style="padding: 100px;">
              <div class="container mt-4">     
                <div class="card border-secondary">  
                    <div class="card-header">Nuevo Registro de Cita</div>
                    <div class="card-body">
                        <form action="/saveCita2" method="POST">
                            <div class="mb-3">
                            <input type="text" placeholder="Nombres del Paciente" class="form-control" id="nombrePaciente" name="nombrePaciente" tabindex="1">
                            </div>   
                            <div class="mb-3">
                              <input type="text" placeholder="Apellidos del Paciente" class="form-control" id="apellidoPaciente" name="apellidoPaciente" tabindex="1">
                            </div>  
                            <div class="mb-3">
                                <input type="text" placeholder="Correo del Paciente" class="form-control" id="correoPaciente" name="correoPaciente" tabindex="1">
                            </div> 
                            <div class="mb-3">
                              <input type="text" placeholder="Nombre del Titular de la Tarjeta" class="form-control" id="nombreTitular" name="nombreTitular" tabindex="1">
                            </div>
                            <div class="mb-3">
                              <input type="number" placeholder="Numero de Tarjeta" class="form-control" id="numTarjeta" name="numTarjeta" tabindex="1">
                            </div>
                            <div class="mb-3">
                              <input type="text" placeholder="Fecha de Expiracion" class="form-control" id="fechaExpiracion" name="fechaExpiracion" tabindex="1">
                            </div>
                            <div class="mb-3">
                              <input type="number" placeholder="CVV" class="form-control" id="cvv" name="cvv" tabindex="1">
                            </div>
                                                        
        
                            <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                            <a href="../index" class="btn btn-secondary">Cancelar</a>
                        </form>
                    </div>
                </div>        
          </div>
